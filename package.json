{
  "name": "bastet-framework",
  "version": "0.0.1",
  "description": "Program Analysis Framework",
  "main": "main.js",
  "scripts": {
    "antlr4ts": "antlr4ts -visitor src/bastet/syntax/parser/grammar/Scratch.g4 -visitor src/bastet/syntax/parser/grammar/ScratchCore.g4",
    "start": "npm run serve",
    "build": "npm run antlr4ts && npm run build-ts && npm run lint && npm run copy-static-assets",
    "serve": "node dist/main.js",
    "watch-node": "nodemon dist/main.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.infosun.fim.uni-passau.de:model-checking/tools/bastet-framework.git"
  },
  "keywords": [
    "program analysis",
    "abstract interpretation",
    "model checking",
    "verification",
    "data flow",
    "testing"
  ],
  "author": "University of Passau, and the contributors",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.17",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "antlr4ts-cli": "^0.5.0-alpha.3",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "shelljs": "^0.8.3",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "acorn": "^7.1.0",
    "antlr4ts": "^0.5.0-alpha.3",
    "commander": "^4.1.0",
    "immutable": "^4.0.0-rc.12",
    "toposort": "^2.0.2",
    "typescript-logging": "^0.6.4"
  }
}
